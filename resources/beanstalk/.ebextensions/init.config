commands: 
  01-get_file:
    command: "aws s3 cp s3://${s3.config.bucket}/sumo.conf /etc/sumo.conf; chmod ugo+rx /etc/sumo.conf;"
  02-create_bridge_log_dir:
    command: "mkdir -p ${bridge.log.dir}; chown -R tomcat:tomcat ${bridge.log.dir};"
  03-create_duracloud_log:
    command: "mkdir -p /var/log/duracloud; chown -R tomcat:tomcat /var/log/duracloud;"
